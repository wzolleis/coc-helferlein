{
  "name": "server",
  "version": "1.0.0",
  "description": "coc server",
  "main": "dist/server.js",
  "private": true,
  "engines": {
    "node": "11.x"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "setup": "npm install",
    "build-ts": "tsc",
    "server": "nodemon dist/src/server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "watch-ts": "tsc -w",
    "test-only": "jest src/",
    "pretest": "npm run build",
    "test": "jest src/",
    "clean": "shx rm -rf node_modules/ dist/ client/node_modules/ client/build/",
    "lint": "tslint --project .",
    "heroku-postbuild": "NPM_PRODUCTION_CONFIG=false npm install --prefix client && npm run build --prefix client && npm run build-ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/request": "^2.48.1",
    "body-parser": "^1.19.0",
    "braces": "^3.0.2",
    "clash-of-clans-api": "^0.6.2",
    "concurrently": "^4.1.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.17.3",
    "har-validator": "^5.1.3",
    "jquery": "^3.4.1",
    "mongoose": "^5.7.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.15.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-session": "^2.0.37",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.10",
    "@types/passport": "^1.0.0",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/supertest": "^2.0.7",
    "js-yaml": "^3.13.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2"
  },
  "heroku-run-build-script": true
}
