{
  "name": "server",
  "version": "1.0.0",
  "description": "coc server",
  "main": "dist/server.js",
  "private": true,
  "engines": {
    "node": "11.x"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "setup": "npm install",
    "build-ts": "tsc",
    "server": "nodemon dist/src/server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "watch-ts": "tsc -w",
    "test-only": "jest src/",
    "pretest": "npm run build",
    "test": "jest src/",
    "clean": "shx rm -rf node_modules/ dist/ client/node_modules/ client/build/",
    "lint": "tslint --project .",
    "heroku-postbuild": "NPM_PRODUCTION_CONFIG=false npm install --prefix client && npm run build --prefix client && npm run build-ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/request": "^2.48.1",
    "body-parser": "^1.18.3",
    "braces": "^2.3.2",
    "clash-of-clans-api": "^0.5.0",
    "concurrently": "^4.1.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "har-validator": "^5.1.3",
    "jquery": "^3.3.1",
    "mongoose": "^5.4.11",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "popper.js": "^1.14.7",
    "react-router-bootstrap": "^0.24.4",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-session": "^2.0.36",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.14",
    "@types/mongoose": "^5.3.12",
    "@types/node": "^10.12.24",
    "@types/passport": "^1.0.0",
    "@types/passport-google-oauth": "^1.0.39",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "shx": "^0.3.2",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "heroku-run-build-script": true
}
